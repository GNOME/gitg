msgid "D_elete file"
msgid_plural "D_elete files"
msgstr[0] "O_dstrániť súbory"
msgstr[1] "O_dstrániť súbor"
msgstr[2] "O_dstrániť súbory"

#: gitg/commit/gitg-commit.vala:1724
